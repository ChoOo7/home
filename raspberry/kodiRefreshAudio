#!/bin/bash
curl 'http://192.168.0.121:8080/jsonrpc' -H 'Pragma: no-cache' -H 'Origin: http://192.168.0.121:8080' -H 'Accept-Encoding: gzip, deflate' -H 'Accept-Language: fr-FR,fr;q=0.8,en-US;q=0.6,en;q=0.4,it;q=0.2' -H 'User-Agent: Mozilla/5.0 (X11; Linux x86_64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/50.0.2661.75 Safari/537.36' -H 'Content-Type: application/json' -H 'Accept: application/json, text/javascript, */*; q=0.01' -H 'Cache-Control: no-cache' -H 'X-Requested-With: XMLHttpRequest' -H 'Connection: keep-alive' -H 'Referer: http://192.168.0.121:8080/' --data-binary '{"jsonrpc":"2.0","method":"AudioLibrary.Scan","id":1}' --compressed